{
  "buildCommand": "echo 'No build needed - static site'",
  "outputDirectory": "services/web",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/services/web/server.py"
    },
    {
      "source": "/modules/(.*)",
      "destination": "/services/web/modules/$1"
    },
    {
      "source": "/styles.css",
      "destination": "/services/web/styles.css"
    },
    {
      "source": "/",
      "destination": "/services/web/index.html"
    }
  ]
}